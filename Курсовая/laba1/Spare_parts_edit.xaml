<Window x:Class="laba1.Window6"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавить запасные части" Height="300" Width="300" Icon="image\shesterenka.png" ResizeMode="NoResize">
    <Grid>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="34,198,0,0" VerticalAlignment="Top" Width="230" Height="50" Click="Add_Click"/>
        <Label Content="Наименование:" HorizontalAlignment="Left" Margin="95,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.449,-0.385"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Height="23" Margin="34,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230"/>
        <Label Content="Количество:" HorizontalAlignment="Left" Margin="104,69,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.449,-0.385"/>
        <TextBox x:Name="col" HorizontalAlignment="Left" Height="23" Margin="34,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230" PreviewTextInput="col_PreviewTextInput"/>
        <Label Content="Стоимость:" HorizontalAlignment="Left" Margin="104,134,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.449,-0.385"/>
        <TextBox x:Name="count" HorizontalAlignment="Left" Height="23" Margin="34,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230" PreviewTextInput="count_PreviewTextInput"/>

    </Grid>
</Window>
